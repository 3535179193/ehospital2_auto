<?xml version="1.0" encoding="UTF-8"?>
<Page name="排班管理">

	<Locator name="排班管理-导入专家排班按钮" 				locatorWay="XPATH"	waitTime="10"	attributeValue="//button[contains(.,'导入专家排班')]"					>schManImportSpecialistScheduleButton</Locator>
	<Locator name="排班管理-导入专家排班号源与挂号费用按钮"	locatorWay="XPATH"	waitTime="10"	attributeValue="//button[contains(.,'导入专家排班号源与挂号费用')]"		>schManImportNumberAndFeeButton</Locator>
	<Locator name="排班管理-导入普通排班按钮" 		locatorWay="XPATH"	waitTime="10"	attributeValue="//button[contains(.,'导入普通排班')]"							>schManImportGeneralScheduleButton</Locator>
	<Locator name="排班管理-下载所有EXCEL模板按钮"	locatorWay="XPATH"	waitTime="10"	attributeValue="//button[contains(.,'下载所有EXCEL模板')]"						>schManDownloadAllExcelButton</Locator>
	<Locator name="排班管理-获取排班按钮" 			locatorWay="XPATH"	waitTime="10"	attributeValue="//button[contains(.,'获取排班')]"								>schManGetScheduleButton</Locator>
	<Locator name="排班管理-批量停诊按钮" 			locatorWay="XPATH"	waitTime="10"	attributeValue="//button[contains(.,'批量停诊')]"								>schManBatchStopMedicalButton</Locator>
	<Locator name="排班管理-批量开诊按钮" 			locatorWay="XPATH"	waitTime="10"	attributeValue="//button[contains(.,'批量开诊')]"								>schManBatchStartMedicalButton</Locator>

	<Locator name="排班管理-科室名称输入框" 		locatorWay="XPATH"	waitTime="10"	attributeValue="//input[@ng-model='$ctrl.param['department_name']']"				>schManDepartmentInputBox</Locator>
	<Locator name="排班管理-医生姓名输入框"		locatorWay="XPATH"	waitTime="10"	attributeValue="//input[@ng-model='$ctrl.param['doctor_name']']"					>schManDoctorInputBox</Locator>
	<Locator name="排班管理-门诊类型下拉框" 		locatorWay="XPATH"	waitTime="10"	attributeValue="//select[@ng-model='$ctrl.param['clinic_type']']"					>schManClinicTypeDropBox</Locator>
	<Locator name="排班管理-选择专家门诊" 		locatorWay="XPATH"	waitTime="10"	attributeValue="//select[@ng-model='$ctrl.param['clinic_type']']/option[2]"			>schManSelectSpecialistClinic</Locator>
	<Locator name="排班管理-选择普通门诊" 		locatorWay="XPATH"	waitTime="10"	attributeValue="//select[@ng-model='$ctrl.param['clinic_type']']/option[3]"			>schManSelectGeneralClinic</Locator>
	<Locator name="排班管理-号源类型下拉框" 		locatorWay="XPATH"	waitTime="10"	attributeValue="//select[@ng-model='$ctrl.param['resource_type']']"					>schManResourceTypeDropBox</Locator>
	<Locator name="排班管理-选择挂号号源" 		locatorWay="XPATH"	waitTime="10"	attributeValue="//select[@ng-model='$ctrl.param['resource_type']']/option[2]"		>schManSelectRegisterResource</Locator>
	<Locator name="排班管理-选择预约号源" 		locatorWay="XPATH"	waitTime="10"	attributeValue="//select[@ng-model='$ctrl.param['resource_type']']/option[3]"		>schManSelectSubscriptionResource</Locator>
	<Locator name="排班管理-搜索按钮" 			locatorWay="XPATH"	waitTime="10"	attributeValue="//button[@ng-click='$ctrl.searchScheduleData()']"					>schManSearchButton</Locator>
	<Locator name="排班管理-导出排班信息按钮" 	locatorWay="XPATH"	waitTime="10"	attributeValue="//button[@ng-click='$ctrl.exportScheduleInfo()']"					>schManExportScheduleInfoButton</Locator>
	
	<Locator name="排班管理-全选按钮" 		locatorWay="XPATH"	waitTime="10"	attributeValue="//input[@ng-model='$ctrl.selectedAllStatus']"							>schManSelectAllButton</Locator>
	<Locator name="排班管理-序号为1的复选框" 	locatorWay="XPATH"	waitTime="10"	attributeValue="//input[@ng-model='item['selected']']"						index="0"	>schManSelectFirstCheckBox</Locator>
	<Locator name="排班管理-序号" 			locatorWay="XPATH"	waitTime="10"	attributeValue="//ul[@ng-repeat='item in $ctrl.scheduleListData']/li[1]"	index="0"	>schManNumber</Locator>
	<Locator name="排班管理-科室名称"		locatorWay="XPATH"	waitTime="10"	attributeValue="//ul[@ng-repeat='item in $ctrl.scheduleListData']/li[2]"	index="0"	>schManDepartmentName</Locator>
	<Locator name="排班管理-地点" 			locatorWay="XPATH"	waitTime="10"	attributeValue="//ul[@ng-repeat='item in $ctrl.scheduleListData']/li[3]"	index="0"	>schManAddress</Locator>
	<Locator name="排班管理-就诊类型" 		locatorWay="XPATH"	waitTime="10"	attributeValue="//ul[@ng-repeat='item in $ctrl.scheduleListData']/li[4]"	index="0"	>schManClinicType</Locator>
	<Locator name="排班管理-医生" 			locatorWay="XPATH"	waitTime="10"	attributeValue="//ul[@ng-repeat='item in $ctrl.scheduleListData']/li[5]"	index="0"	>schManDoctor</Locator>
	<Locator name="排班管理-就诊时间" 		locatorWay="XPATH"	waitTime="10"	attributeValue="//ul[@ng-repeat='item in $ctrl.scheduleListData']/li[6]"	index="0"	>schManClinicTime</Locator>
	<Locator name="排班管理-时间段" 			locatorWay="XPATH"	waitTime="10"	attributeValue="//ul[@ng-repeat='item in $ctrl.scheduleListData']/li[7]"	index="0"	>schManClinicTimePeriod</Locator>
	<Locator name="排班管理-号源" 			locatorWay="XPATH"	waitTime="10"	attributeValue="//ul[@ng-repeat='item in $ctrl.scheduleListData']/li[8]"	index="0"	>schManResourse</Locator>
	<Locator name="排班管理-号源类型" 		locatorWay="XPATH"	waitTime="10"	attributeValue="//ul[@ng-repeat='item in $ctrl.scheduleListData']/li[9]"	index="0"	>schManResourseType</Locator>

	<Locator name="排班管理-查看"			locatorWay="XPATH"	waitTime="10"	attributeValue="//ul[@ng-repeat='item in $ctrl.scheduleListData']/li[10]/div/a[1]"	index="0"	>schManViewButton</Locator>
		<Locator name="排班信息-排班ID" 		locatorWay="XPATH"	waitTime="10"	attributeValue="//input[@ng-model='$ctrl.scheduleDetail['schedule_id']']"							>schInfDepartmentID</Locator>
		<Locator name="排班信息-科室" 		locatorWay="XPATH"	waitTime="10"	attributeValue="//input[@ng-model='$ctrl.scheduleDetail['department_name']']"						>schInfDepartmentName</Locator>
		<Locator name="排班信息-就诊类型" 	locatorWay="XPATH"	waitTime="10"	attributeValue="//input[@ng-model='$ctrl.scheduleDetail['clinic_type']']"							>schInfClinicType</Locator>
		<Locator name="排班信息-医生" 		locatorWay="XPATH"	waitTime="10"	attributeValue="//input[@ng-model='$ctrl.scheduleDetail['doctor_name']']"							>schInfDoctor</Locator>
		<Locator name="排班信息-号源" 		locatorWay="XPATH"	waitTime="10"	attributeValue=""															>schInfResource</Locator>
		<Locator name="排班信息-就诊时间" 	locatorWay="XPATH"	waitTime="10"	attributeValue="//input[@ng-model='$ctrl.searchDoctorStr']"											>schInfClinicTime</Locator>
		<Locator name="排班信息-保存按钮" 	locatorWay="XPATH"	waitTime="10"	attributeValue="//button[@type='submit']"															>schInfSaveButton</Locator>
		<Locator name="排班信息-取消按钮" 	locatorWay="XPATH"	waitTime="10"	attributeValue="//button[@ng-click='$ctrl.cancelHandle()']"											>schInfCancelButton</Locator>
	
	<Locator name="排班管理-停诊" 			locatorWay="XPATH"	waitTime="10"	attributeValue="//ul[@ng-repeat='item in $ctrl.scheduleListData']/li[10]/div/a[2]"	index="0"	>schManStopMedicalButton</Locator>
	<Locator name="排班管理-日志" 			locatorWay="XPATH"	waitTime="10"	attributeValue="//ul[@ng-repeat='item in $ctrl.scheduleListData']/li[10]/div/a[3]"	index="0"	>schManViewLogButton</Locator>
	
</Page>