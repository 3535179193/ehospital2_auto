<?xml version="1.0" encoding="UTF-8"?>
<Page name="科室管理">

	<Locator name="科室管理-导入科室信息按钮" 		locatorWay="XPATH"	waitTime="10"	attributeValue="//button[contains(.,'导入科室信息')]"				>depManImportDepartmentInfoButton</Locator>
	<Locator name="科室管理-下载科室表Excel模板按钮" 	locatorWay="XPATH"	waitTime="10"	attributeValue="//button[contains(.,'下载科室表Excel模板']"			>depManDownloadDepartmentExcelButton</Locator>
	<Locator name="科室管理-导出科室信息按钮"			locatorWay="XPATH"	waitTime="10"	attributeValue="//button[contains(.,'导出科室信息')]"				>depManExportDepartmentInfoButton</Locator>
	<Locator name="科室管理-同步科室HIS数据按钮" 		locatorWay="XPATH"	waitTime="10"	attributeValue="//button[contains(.,'同步HIS数据')]"					>depManSynchronousDepartmentHISButton</Locator>
    
	<Locator name="科室管理-科室名称输入框" 			locatorWay="XPATH"	waitTime="10"	attributeValue="//input[@ng-model='$ctrl.searchDepartmentStr']"		>depManDepartmentNameInputBox</Locator>
	<Locator name="科室管理-搜索科室按钮" 			locatorWay="XPATH"	waitTime="10"	attributeValue="//button[contains(.,'搜索')]"						>depManSearchDepartmentButton</Locator>
	
	<Locator name="科室管理-新增一级科室按钮"			locatorWay="XPATH"	waitTime="10"	attributeValue="//button[contains(.,'新增一级科室')]"				>depManAddFirstDepartmentButton</Locator>
		<Locator name="新增一级科室-科室名称输入框" 	locatorWay="XPATH"	waitTime="10"	attributeValue="//input[@ng-model='$ctrl.firstDepartmentData.category_name']"		>addCateDepartmentNameInputBox</Locator>
		<Locator name="新增一级科室-科室编码输入框" 	locatorWay="XPATH"	waitTime="10"	attributeValue="//input[@ng-model='$ctrl.firstDepartmentData.category_code']"		>addCateDepartmentCodeInputBox</Locator>
		<Locator name="新增一级科室-科室LOGO按钮" 	locatorWay="XPATH"	waitTime="10"	attributeValue="//div[@ng-click='$ctrl.selectFileHandle()']"						>addCateDepartmentLogoButton</Locator>
		<Locator name="新增一级科室-保存按钮" 		locatorWay="XPATH"	waitTime="10"	attributeValue="//button[@ng-if='$ctrl.isAdd']"										>addCateSaveButton</Locator>
		<Locator name="新增一级科室-返回按钮" 		locatorWay="XPATH"	waitTime="10"	attributeValue="//button[contains(.,'返回')]"										>addCateReturnButton</Locator>
	
	<Locator name="科室管理-序号" 				locatorWay="XPATH"	waitTime="10"	attributeValue="//li[@ng-bind='$index+1']"					index="0"	>depManNumber</Locator>
	<Locator name="科室管理-序号1的一级科室名称" 		locatorWay="XPATH"	waitTime="10"	attributeValue="//li[@ng-bind='item.category_name']"		index="0"	>depManFirstCategoryName</Locator>
	<Locator name="科室管理-序号2的一级科室名称" 		locatorWay="XPATH"	waitTime="10"	attributeValue="//li[@ng-bind='item.category_name']"		index="1"	>depManSecondCategoryName</Locator>
	<Locator name="科室管理-包含的二级科室" 		locatorWay="XPATH"	waitTime="10"	attributeValue="//li[@ng-bind='item.department_name']"		index="0"	>depManContainDepartments</Locator>

	<Locator name="科室管理-编辑按钮" 			locatorWay="XPATH"	waitTime="10"	attributeValue="//li[contains(.,'睿博一级科室')]/parent::ul/li[4]/div/div[1]/a[1]"		>depManEditButton</Locator>
  		<Locator name="编辑一级科室-科室名称输入框" 	locatorWay="XPATH"	waitTime="10"	attributeValue="//input[@ng-model='$ctrl.firstDepartmentData.category_name']"		>ediCateDepartmentNameInputBox</Locator>
		<Locator name="编辑一级科室-科室编码输入框" 	locatorWay="XPATH"	waitTime="10"	attributeValue="//input[@ng-model='$ctrl.firstDepartmentData.category_code']"		>ediCateDepartmentCodeInputBox</Locator>
		<Locator name="编辑一级科室-科室LOGO按钮" 	locatorWay="XPATH"	waitTime="10"	attributeValue="//div[@ng-click='$ctrl.selectFileHandle()']"						>ediCateDepartmentLogoButton</Locator>
		<Locator name="编辑一级科室-修改按钮" 		locatorWay="XPATH"	waitTime="10"	attributeValue="//button[@ng-if='!$ctrl.isAdd']"										>ediCateEditButton</Locator>
		<Locator name="编辑一级科室-返回按钮" 		locatorWay="XPATH"	waitTime="10"	attributeValue="//button[contains(.,'返回')]"										>ediCateReturnButton</Locator>
	
    <Locator name="科室管理-删除按钮" 			locatorWay="XPATH"	waitTime="10"	attributeValue="//li[contains(.,'睿博一级科室（改）')]/parent::ul/li[4]/div/div[1]/a[2]"		>depManDeleteButton</Locator>
	<Locator name="科室管理-查看二级科室按钮" 	locatorWay="XPATH"	waitTime="10"	attributeValue="//a[contains(.,'查看二级科室')]"				index="0"	>depManViewDepartmentButton</Locator>
	<Locator name="科室管理-上移按钮" 			locatorWay="XPATH"	waitTime="10"	attributeValue="//a[contains(.,'上移')]"						index="1"	>depManMoveUpButton</Locator>
    <Locator name="科室管理-下移按钮" 			locatorWay="XPATH"	waitTime="10"	attributeValue="//a[contains(.,'下移')]"						index="0"	>depManMoveDownButton</Locator>
	<Locator name="科室管理-置顶按钮" 			locatorWay="XPATH"	waitTime="10"	attributeValue="//li[contains(.,'睿博一级科室')]/parent::ul/li[4]/div/div[2]/a[3]"		>depManMoveTopButton</Locator>

	<Locator name="科室管理-每页显示数下拉框" 	locatorWay="XPATH"	waitTime="10"	attributeValue="//select[@ng-model='$ctrl.pageEvery']"							>depManEachPageRecordersDropBox</Locator>
	<Locator name="科室管理-选中每页显示10条" 	locatorWay="XPATH"	waitTime="10"	attributeValue="//select[@ng-model='$ctrl.pageEvery']/option"	index="0"		>depManEachPageTenRecorders</Locator>
	<Locator name="科室管理-总记录数" 			locatorWay="XPATH"	waitTime="10"	attributeValue="//div[@class='department-management']/page/div/div[2]/span[1]"	>depManTotalRecorders</Locator>
	<Locator name="科室管理-总页数" 				locatorWay="XPATH"	waitTime="10"	attributeValue="//span[@ng-bind='$ctrl.pageTotal']"								>depManTotalPages</Locator>
	<Locator name="科室管理-当前页数" 			locatorWay="XPATH"	waitTime="10"	attributeValue="//div[@class='department-management']/page/div/div[2]/span[3]"	>depManCurrentPage</Locator>
	<Locator name="科室管理-第一页按钮"		 	locatorWay="XPATH"	waitTime="10"	attributeValue="//div[@class='department-management']/page/div/div[2]/a[1]"		>depManFirstPageButton</Locator>
	<Locator name="科室管理-上一页按钮"		 	locatorWay="XPATH"	waitTime="10"	attributeValue="//div[@class='department-management']/page/div/div[2]/a[2]"		>depManPreviousPageButton</Locator>	
	<Locator name="科室管理-下一页按钮"			locatorWay="XPATH"	waitTime="10"	attributeValue="//div[@class='department-management']/page/div/div[2]/a[3]"		>depManNextPageButton</Locator>
	<Locator name="科室管理-最末页按钮"			locatorWay="XPATH"	waitTime="10"	attributeValue="//div[@class='department-management']/page/div/div[2]/a[4]"		>depManLastPageButton</Locator>

	<Locator name="科室管理-弹框关闭按钮"			locatorWay="XPATH"	waitTime="10"	attributeValue="//div[@ng-show='$ctrl.showCloseButton']/button"				>depManCloseButton</Locator>
	
	<Locator name="科室管理-被验证的新增一级科室名称"			locatorWay="XPATH"	waitTime="10"	attributeValue="//li[contains(.,'睿博一级科室')]"						>depManAssertAddDepartmentCategory</Locator>
	
</Page>