	<Locator name="已添加星级评价项-置顶按钮" 		locatorWay="XPATH"	waitTime="10"	attributeValue="//ul[@ng-repeat='stars in $ctrl.evaluationItemListData | filter:{'item_type':'0'}']/li[4]/div/a[4]"		index="0"	>appIteStatus</Locator>	
	-->
													
	<Locator name="已添加文本评价项-序号" 			locatorWay="XPATH"	waitTime="10"	attributeValue="//li[contains(.,'睿博文本评价项')]/parent::ul/li[1]"				index="0"	>appIteTextNumber</Locator>
	<Locator name="已添加文本评价项-名称" 			locatorWay="XPATH"	waitTime="10"	attributeValue="//li[contains(.,'睿博文本评价项')]"								index="0"	>appIteTextName</Locator>
	<Locator name="已添加文本评价项-类型" 			locatorWay="XPATH"	waitTime="10"	attributeValue="//li[contains(.,'睿博文本评价项')]/parent::ul/li[3]"				index="0"	>appIteTextType</Locator>
	<Locator name="已添加文本评价项-移除按钮" 		locatorWay="XPATH"	waitTime="10"	attributeValue="//li[contains(.,'睿博文本评价项')]/parent::ul/li[4]/div/a[1]"	index="0"	>appIteTextDeleteButton</Locator>
	
