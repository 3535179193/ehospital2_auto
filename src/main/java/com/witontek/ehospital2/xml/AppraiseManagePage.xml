<?xml version="1.0" encoding="UTF-8"?>
<Page name="就医评价管理">

	<Locator name="就医评价管理-问卷名称输入框" 		locatorWay="XPATH"	waitTime="10"	attributeValue="//input[@ng-model='$ctrl.searchEvaluationQuestionnaireStr']"	>appManAppraiseNameInputBox</Locator>
	<Locator name="就医评价管理-搜索按钮" 			locatorWay="XPATH"	waitTime="10"	attributeValue="//button[@ng-click='$ctrl.searchEvaluation()']"					>appManSearchButton</Locator>

	<Locator name="就医评价管理-新增评价问卷按钮"		locatorWay="XPATH"	waitTime="10"	attributeValue="//button[@ng-click='$ctrl.openEvaluationModal(true)']"			>appManAddAppraiseButton</Locator>
		<Locator name="评价问卷框-问卷名称输入框"	locatorWay="XPATH"	waitTime="10"	attributeValue="//input[@name='appraise_name']"									>appBoxAppraiseNameInputBox</Locator>
		<Locator name="评价问卷框-问卷说明输入框"	locatorWay="XPATH"	waitTime="10"	attributeValue="//textarea[@name='appraise_desc']"								>appBoxAppraiseDescInputBox</Locator>
		<Locator name="评价问卷框-问卷类型下拉框" 	locatorWay="XPATH"	waitTime="10"	attributeValue="//select[@name='appraise_type']"								>appBoxAppraiseTypeDropBox</Locator>																																									 
		<Locator name="评价问卷框-选择问卷类型按钮" locatorWay="XPATH"	waitTime="10"	attributeValue="//select[@name='appraise_type']/option[2]"						>appBoxAppraiseTypeButton</Locator>
		<Locator name="评价问卷框-草稿问卷状态按钮"	locatorWay="XPATH"	waitTime="10"	attributeValue="//label[contains(.,'草稿')]"										>appBoxDraftStatusButton</Locator>
		<Locator name="评价问卷框-发布问卷状态按钮"	locatorWay="XPATH"	waitTime="10"	attributeValue="//label[contains(.,'发布')]"										>appBoxPostedStatusButton</Locator>
		<Locator name="评价问卷框-暂停问卷状态按钮"	locatorWay="XPATH"	waitTime="10"	attributeValue="//label[contains(.,'暂停')]"										>appBoxStopStatusButton</Locator>
		<Locator name="评价问卷框-保存按钮" 		locatorWay="XPATH"	waitTime="10"	attributeValue="//button[contains(.,'保存')]"									>appBoxSaveButton</Locator>
		<Locator name="评价问卷框-保存后继续添加按钮" locatorWay="XPATH"	waitTime="10"	attributeValue="//button[@ng-if='$ctrl.isAdd']"								>appBoxSaveAndAddButton</Locator>
		<Locator name="评价问卷框-取消按钮" 		locatorWay="XPATH"	waitTime="10"	attributeValue="//button[@ng-click='$ctrl.cancelHandle()']"						>appBoxCancleButton</Locator>
							
	<Locator name="就医评价管理-序号" 				locatorWay="XPATH"	waitTime="10"	attributeValue="//ul[@ng-repeat='item in $ctrl.evaluationListData']/li[1]"		index="0"	>appManNumber</Locator>
	<Locator name="就医评价管理-评价问卷名称" 		locatorWay="XPATH"	waitTime="10"	attributeValue="//ul[@ng-repeat='item in $ctrl.evaluationListData']/li[2]"		index="0"	>appManAppraiseName</Locator>
	<Locator name="就医评价管理-评价问卷类型" 		locatorWay="XPATH"	waitTime="10"	attributeValue="//ul[@ng-repeat='item in $ctrl.evaluationListData']/li[3]"		index="0"	>appManAppraiseType</Locator>
	<Locator name="就医评价管理-创建人" 				locatorWay="XPATH"	waitTime="10"	attributeValue="//ul[@ng-repeat='item in $ctrl.evaluationListData']/li[4]"		index="0"	>appManAuthor</Locator>
	<Locator name="就医评价管理-创建日期" 			locatorWay="XPATH"	waitTime="10"	attributeValue="//ul[@ng-repeat='item in $ctrl.evaluationListData']/li[5]"		index="0"	>appManCreateDate</Locator>
	<Locator name="就医评价管理-更新日期" 			locatorWay="XPATH"	waitTime="10"	attributeValue="//ul[@ng-repeat='item in $ctrl.evaluationListData']/li[6]"		index="0"	>appManUpdateDate</Locator>
	<Locator name="就医评价管理-状态" 				locatorWay="XPATH"	waitTime="10"	attributeValue="//ul[@ng-repeat='item in $ctrl.evaluationListData']/li[7]"		index="0"	>appManStatus</Locator>
	<Locator name="就医评价管理-填写数" 				locatorWay="XPATH"	waitTime="10"	attributeValue="//ul[@ng-repeat='item in $ctrl.evaluationListData']/li[8]"		index="0"	>appManFillNumber</Locator>
	<Locator name="就医评价管理-删除按钮" 			locatorWay="XPATH"	waitTime="10"	attributeValue="//li[contains(.,'睿博医院评价')]/parent::ul/li[9]/div/a[2]"		index="0"	>appManDeleteButton</Locator>

	<Locator name="就医咨询管理-编辑按钮" 			locatorWay="XPATH"	waitTime="10"	attributeValue="//li[contains(.,'睿博医院评价')]/parent::ul/li[9]/div/a[1]"		index="0"	>appManEditButton</Locator>
		<Locator name="评价问卷框-编辑评价项按钮"	locatorWay="XPATH"	waitTime="10"	attributeValue="//button[@ng-if='!$ctrl.isAdd']"											>appBoxEditAppraiseItemButton</Locator>

<!--		
	<Locator name="就医评价管理-被验证的评价问卷名称"	locatorWay="XPATH"	waitTime="10"	attributeValue="//li[contains(.,'睿博一级科室')]"				>appManAssertAddConsultDepartmentName</Locator>
	<Locator name="就医评价管理-被验证的评价问卷名称"	locatorWay="XPATH"	waitTime="10"	attributeValue="//li[contains(.,'睿博一级科室(改)')]"			>appManAssertEditConsultDepartmentName</Locator>
-->
</Page>