		<Locator name="新增评价问卷-选择问卷类型按钮" locatorWay="XPATH"	waitTime="10"	attributeValue="//select[@name='appraise_type']/option[2]"						>addAppAppraiseTypeButton</Locator>
		<Locator name="新增评价问卷-草稿问卷状态按钮"	locatorWay="XPATH"	waitTime="10"	attributeValue="//label[contains(.,'草稿')]"										>addAppDraftStatusButton</Locator>
